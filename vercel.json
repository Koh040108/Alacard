{
    "version": 2,
    "framework": "vite",
    "installCommand": "npm install && cd backend && npx prisma generate && cd ../frontend && npm install",
    "buildCommand": "cd frontend && npm run build",
    "outputDirectory": "frontend/dist",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api"
        }
    ]
}